{
  "name": "siripravi/yii2-slideradmin",
  "description": "Multilingual Slider image administration for Yii2 framework",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "slider",
    "carousel",
    "multilingual",
    "image upload"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Pravinya",
      "email": "provdigi@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap5": "~2.0.2",
    "omgdef/yii2-multilingual-behavior": "~2.1",
    "2amigos/yii2-ckeditor-widget": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "siripravi\\slideradmin\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
